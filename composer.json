{
	"name": "friendsoftypo3/kickstarter",
	"description": "Kickstart TYPO3 Extension",
	"license": "GPL-2.0-or-later",
	"type": "typo3-cms-extension",
	"keywords": [
		"typo3",
		"TYPO3 CMS",
		"extbase",
		"builder",
		"kickstart",
		"dev"
	],
	"authors": [
		{
			"name": "Kickstarter Development Team",
			"email": "friendsof@typo3.org",
			"role": "Developer"
		},
		{
			"name": "Stefan Froemken",
			"email": "froemken@gmail.com",
			"role": "Lead Developer"
		},
		{
			"name": "Lina Wolf",
			"email": "112@linawolf.de",
			"role": "Maintainer"
		}
	],
	"homepage": "https://extensions.typo3.org/extension/kickstarter",
	"support": {
		"email": "friendsof@typo3.org",
		"issues": "https://github.com/FriendsOfTYPO3/kickstarter/issues",
		"source": "https://github.com/FriendsOfTYPO3/kickstarter",
		"docs": "https://docs.typo3.org/p/friendsoftypo3/kickstarter/main/en-us/Index.html"
	},
	"require": {
		"php": "^8.2",
		"ext-curl": "*",
		"ext-dom": "*",
		"ext-libxml": "*",
		"ext-pdo": "*",
		"ext-zip": "*",
		"friendsofphp/php-cs-fixer": "^3.49",
		"typo3/cms-core": "^13.4.3",
		"typo3/cms-install": "^13.4.3"
	},
	"require-dev": {
		"ergebnis/composer-normalize": "^2.44",
		"phpstan/phpstan": "^2.1.25",
		"ssch/typo3-rector": "^3.6",
		"typo3/coding-standards": "^0.8",
		"typo3/testing-framework": "^8.2"
	},
	"conflict": {
		"b13/make": "*"
	},
	"autoload": {
		"psr-4": {
			"FriendsOfTYPO3\\Kickstarter\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"FriendsOfTYPO3\\Kickstarter\\Tests\\": "Tests"
		}
	},
	"config": {
		"allow-plugins": {
			"ergebnis/composer-normalize": true,
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		},
		"bin-dir": ".Build/bin",
		"sort-packages": true,
		"vendor-dir": ".Build/vendor"
	},
	"extra": {
		"typo3/cms": {
			"app-dir": ".Build",
			"extension-key": "kickstarter",
			"web-dir": ".Build/public"
		}
	}
}
